{
  "manifest_version": 3,
  "name": "PlanYt",
  "version": "1.0.0",
  "description": "Your Youtube, planned.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://*.vercel.app/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "assets/icon.png"
    }
  },
  "icons": {
    "128": "assets/icon.png"
  }
}
